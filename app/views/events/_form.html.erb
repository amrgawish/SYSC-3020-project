  </p>
   <p>
    <%= t.label :start_time %><br>
    <%= t.text_field :start_time %>
  </p>
   <p>
    <%= t.label :end_time %><br>
    <%= t.text_field :end_time %>
  </p>
  <p>
    <%= t.label :description %><br>
    <%= t.text_field :description %>
  </p>
  <p>
    <%= t.submit%>
  </p>
<% end %>
