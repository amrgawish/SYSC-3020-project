
<%= form_for @event do |t| %>
  <% if @event.errors.any? %>
    <div id ="error_explanation">
      <h2>
        <%=pluralize(@event.errors.count, "error") %> prohibited
        this article from being saved:
      </h2>
      <ul>
        <% @event.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <p>
    <%= t.label :title %><br>
    <%= t.text_field :name %>
  </p>
   
   <p>
    <%= t.label :class %><br>
